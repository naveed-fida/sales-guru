{
  "name": "sales_guru",
  "version": "1.0.2",
  "description": "An Electron & React application with Sqlite, Prisma and Typescript",
  "main": "./dist/main/index.js",
  "repository": "https://github.com/naveed-fida/sales-guru",
  "author": "Naveed Fida",
  "scripts": {
    "build": "electron-vite build && prisma generate && tsc --noEmit && node copy-files.js && node install-engines-on-mac.js",
    "start": "npm run build && concurrently \"vite\" \"cross-env NODE_ENV=development electron .\"",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "publish": "npm run build && dotenv -- electron-builder -p always"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^2.0.0",
    "@electron-toolkit/utils": "^2.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@mui/material": "^5.14.17",
    "@prisma/client": "^5.2.0",
    "@prisma/engines": "^5.6.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.1",
    "formik": "^2.4.3",
    "prisma": "^5.2.0",
    "react-datepicker": "^4.21.0",
    "react-router-dom": "^6.15.0",
    "replace-in-file": "^7.0.2",
    "tiny-invariant": "^1.3.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^1.0.1",
    "@electron-toolkit/eslint-config-ts": "^1.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@faker-js/faker": "^8.3.1",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.2",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.27",
    "eslint": "^8.47.0",
    "eslint-plugin-react": "^7.33.2",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
